{
  "name": "ula-starter",
  "version": "1.0.3",
  "main": "dist-electron/main/index.js",
  "description": "Uipath log analyzer starter.",
  "author": "HinokiSu",
  "license": "MIT",
  "private": true,
  "repository": {
    "url": "https://github.com/HinokiSu/ula-starter"
  },
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "Uipath",
    "parse-log"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "set NODE_ENV=development&&vite",
    "build": "set NODE_ENV=production&&vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "build:font": "set NODE_ENV=production&&vue-tsc --noEmit && vite build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "jest": "^29.5.0",
    "less": "^4.1.3",
    "pinia": "^2.0.33",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.12.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.1.7"
  },
  "dependencies": {
    "vue-i18n": "^9.2.2"
  }
}
